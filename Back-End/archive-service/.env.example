# Archive Service Configuration
SERVICE_NAME=archive-service
PORT=8001

# Database Configuration
DATABASE_URL=postgresql+asyncpg://omnia:omnia_password@postgres:5432/omnia_archive

# RabbitMQ Configuration
RABBITMQ_URL=amqp://guest:guest@rabbitmq:5672/
EMBEDDING_QUEUE_NAME=embedding_queue

# MinIO Configuration
MINIO_ENDPOINT=minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=omnia-archive
MINIO_SECURE=false

# File Upload Configuration
MAX_FILE_SIZE_MB=100
ALLOWED_FILE_TYPES=["pdf", "docx", "txt", "md", "png", "jpg", "jpeg", "gif", "mp4", "mp3"]

# Instagram Configuration
INSTAGRAM_SESSION_FILE=/app/data/instagram_session
